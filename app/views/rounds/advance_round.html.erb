<div class="font-effect-anaglyph">Advance Round</div>
<div class="form_field">
  <%= form_for(@newrnd, url: save_score_path(@newrnd.id)) do |f| %>
    <%= f.fields_for :performances do |p| %>
      <%= p.object.poet.name %>
      <div class="field">
        <%= p.label :score1 %>
        <%= p.text_field :score1 %>
      </div>
      <div class="field">
        <%= p.label :score2 %>
        <%= p.text_field :score2 %>
      </div>
      <div class="field">
        <%= p.label :score3 %>
        <%= p.text_field :score3 %>
      </div>
      <div class="field">
        <%= p.label :score4 %>
        <%= p.text_field :score4 %>
      </div>
      <div class="field">
        <%= p.label :score5 %>
        <%= p.text_field :score5 %>
      </div>
    <% end %>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
</div>

<div class="bottom_link"><%= link_to 'Back', rounds_path %></div>
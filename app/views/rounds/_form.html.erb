<div class="form_field">
  <%= form_for(@round) do |f| %>
    <div class="field">
      <%= f.label :slam_id %><br />
      <%= f.number_field :slam_id %>
    </div>
    <div class="field">
      <%= f.label :round_number %><br />
      <%= f.number_field :round_number %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
</div>
